<script type="text/javascript">
	var x1 = +prompt("x1");
	var y1 = +prompt("y1");
	var x2 = +prompt("x2");
	var y2 = +prompt("y2");
	var k = (y1 - y2)/(x1 - x2);
	var b = y2 - k*x2;
	document.write("y = "+k+"x +" + b);
</script>
